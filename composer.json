{
    "name": "bogdanfinn/tmdb-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "Symfony Bundle for accessing https://www.themoviedb.org API",
    "homepage": "https://github.com/bogdanfinn/tmdb-bundle",
    "keywords": ["tmdb", "api", "symfony","bundle"],
    "authors": [
        {
            "name": "Bogdan Finn",
            "homepage": "https://www.bogdanfinn.de",
            "email": "kontakt@bogdanfinn.de"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "symfony/config": ">=2.3,<4",
        "symfony/dependency-injection": ">=2.3,<4",
        "symfony/event-dispatcher": ">=2.3,<4",
        "symfony/http-kernel": ">=2.3,<4",
        "twig/twig": "~1.11|~2.0",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "symfony/framework-bundle": ">=2.3,<4"
    },
    "autoload": {
        "psr-4": { "bogdanfinn\\tmdbBundle\\": "" }
    }
